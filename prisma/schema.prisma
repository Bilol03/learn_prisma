generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model User {
  id             String        @id @default(cuid())
  phone          String?       
  firstName      String?
  lastName       String?
  password       String?
  curentBranch   String?
  createdAt      DateTime      @default(now())
  updatedAt      DateTime      @default(now())

  @@map("users")
}

